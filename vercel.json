{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "env": {
    "ULTRAMSG_TOKEN": "@ultramsg_token",
    "ULTRAMSG_INSTANCE_ID": "@ultramsg_instance_id"
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true,
      "development": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true
  }
} 