  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:production": "node scripts/build-production.js",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "postinstall": "prisma generate && node -e \"const fs=require('fs');['postcss.config.js','tailwind.config.js'].forEach(f=>!fs.existsSync(f)&&fs.writeFileSync(f,''))\"",
    "migrate": "node -r dotenv/config scripts/run-migration.js",
    "cleanup:logs": "ts-node scripts/replace-console-logs.ts",
    "test:db": "ts-node -r dotenv/config scripts/test-db-connection.ts",
    "generate:favicons": "node scripts/generate-favicons.js && node scripts/create-root-favicon.js && node scripts/sync-favicons.js",
    "generate:admin-icons": "node scripts/generate-admin-icons.js",
    "generate:admin-screenshot": "node scripts/generate-admin-screenshot.js",
    "fix:booking-dates": "ts-node -r dotenv/config scripts/fix-booking-dates.ts",
    "test:notifications": "ts-node -r dotenv/config scripts/test-notifications.ts",
    "maintenance": "ts-node -r dotenv/config scripts/maintenance-mode.ts",
    "maintenance:on": "ts-node -r dotenv/config scripts/maintenance-mode.ts on",
    "maintenance:off": "ts-node -r dotenv/config scripts/maintenance-mode.ts off",
    "maintenance:status": "ts-node -r dotenv/config scripts/maintenance-mode.ts status",
    "test:redirect": "ts-node -r dotenv/config scripts/test-redirect.ts",
    "test:middleware": "ts-node -r dotenv/config scripts/check-middleware.ts",
    "setup:settings": "ts-node -r dotenv/config scripts/setup-settings.ts"
  },
