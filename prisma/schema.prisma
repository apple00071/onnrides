generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id            String   @id @default(cuid())
  name          String
  description   String?
  image_url     String
  price_per_day Decimal
  location      String
  address       String
  type          String
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
}
